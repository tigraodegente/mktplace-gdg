{
  "name": "@mktplace/xata-client",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "generate": "xata codegen --output ./src/xata.ts"
  },
  "keywords": [
    "xata",
    "database",
    "client",
    "marketplace"
  ],
  "author": "Gustavo",
  "license": "ISC",
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
    "@xata.io/client": "^0.30.1"
  },
  "devDependencies": {
    "@mktplace/shared-types": "workspace:*",
    "@xata.io/cli": "^0.16.0",
    "typescript": "^5.0.0"
  }
}
